{
    "name": "jnjxp/vk",
    "description": "Voight-Kampff ADR Authentication module",
    "homepage":"https://github.com/jnjxp/jnjxp.vk",
    "license": "AGPL",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "aura/payload": "^3.0",
        "aura/auth": "^2.0",
        "vperyod/auth-handler":"dev-develop"
    },
    "require-dev":{
        "zendframework/zend-diactoros": "~1.0",
        "radar/adr":"^1.0",
        "fusible/auth-provider" : "dev-develop",
        "vperyod/session-handler":"dev-develop",
        "vperyod/accept-handler":"dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Jnjxp\\Vk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jnjxp\\Vk\\":"tests/",
            "Aura\\Di\\": "vendor/aura/di/tests/",
            "Vperyod\\SessionHandler\\": "vendor/vperyod/session-handler/tests/"
        }
    },
    "suggest": {
        "vperyod/session-handler":"for responders",
        "vperyod/accept-handler":"for responders"
    },
    "minimum-stability": "dev"
}
