{
    "name": "jnjxp/vk",
    "description": "Voight-Kampff Authentication module",
    "homepage": "https://github.com/jnjxp/jnjxp.vk",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "fig/http-message-util": "^1.1",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "laminas/laminas-diactoros": "^2.1",
        "mezzio/mezzio": "^3.2",
        "mezzio/mezzio-authentication": "^1.1",
        "mezzio/mezzio-authentication-session": "^1.0",
        "mezzio/mezzio-flash": "^1.0",
        "mezzio/mezzio-session": "^1.2",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "paulthebaud/phpunit-generator": "^2.1",
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Jnjxp\\Vk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jnjxp\\Vk\\": "tests/"
        }
    },
    "extra": {
        "laminas": {
            "config-provider": [
                "Jnjxp\\Vk\\ConfigProvider"
            ]
        }
    }
}
